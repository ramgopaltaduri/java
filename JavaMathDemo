public class JavaMathDemo
{
    public static void main(String args[])
    {
        double x = 28;
        double y = 4;
        System.out.println("maximum of x and y is "+Math.max(x,y));
        System.out.println("minimum of x and y is "+Math.min(y,x));
        System.out.println("Square root of x is "+Math.sqrt(x));
        System.out.println("logarithm of x is "+Math.log(x));
        System.out.println("log 10 of x is "+Math.log10(x));
        System.out.println("round of 4.56 is"+Math.round(4.56));
    }
}
